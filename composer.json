{
    "name": "sunjim/laravel-module",
    "description": "laravel modules",
    "license": "MIT",
    "authors": [{
        "name": "sunjim",
        "email": "275980239@qq.com"
    }],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "\\Houdunwang\\Module\\LaravelServiceProvider"
            ],
            "aliases": {
                "HDModule": "\\Houdunwang\\Module\\Factory"
            }
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "nwidart/laravel-modules": "^5.0",
        "spatie/laravel-permission": "^2.36.1",
        "houdunwang/laravel-upload": "^1.0",
        "doctrine/dbal": "^2.9.2",
        "houdunwang/laravel-autocreate": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Houdunwang\\Module\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "houdunwang\\Module\\Tests\\": "tests/"
        }
    }
}